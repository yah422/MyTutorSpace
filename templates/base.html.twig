        <meta charset="UTF-8">
        <title>{% block title %}Bienvenue !{% endblock %}</title>
        <link rel="icon" href="{{ asset('img/iconTitle.png') }}">
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
